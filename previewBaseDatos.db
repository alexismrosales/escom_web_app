USE `alumnos_escom`;
CREATE TABLE `alumno` (
    `boleta` varchar(10) NOT NULL,
    `nombre` varchar(64) NOT NULL,
    `apellido_paterno` varchar(64) NOT NULL,
    `apellido_materno` varchar(64) NOT NULL,
    `fecha_nacimiento` date DEFAULT NOT NULL,
    `genero` char NOT NULL,
    `curp` varchar(18) NOT NULL,
    `calle` varchar(30) NOT NULL,
    `numero` varchar(30) NOT NULL,
    `colonia` varchar(30) NOT NULL,
    `alcaldia` varchar(30) NOT NULL,
    `cp` int NOT NULL,
    `telefono` bigint NOT NULL,
    `email` varchar(64) NOT NULL,
    `escuela_procedencia` varchar(64) NOT NULL,
    `estado` varchar(30) NOT NULL,
    `promedio` float NOT NULL,
    `numero_opcion` smallint NOT NULL,
    `laboratorio_asignado` varchar(10) NOT NULL,
    `horario_asignado` datetime NOT NULL,
    PRIMARY KEY(`boleta`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;

